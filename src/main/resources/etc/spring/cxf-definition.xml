<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jaxrs="http://cxf.apache.org/jaxrs" xmlns:cxf="http://cxf.apache.org/core"
	xmlns:jaxws="http://cxf.apache.org/jaxws" xmlns="http://www.springframework.org/schema/beans"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/util 
	http://www.springframework.org/schema/util/spring-util.xsd 
	http://cxf.apache.org/jaxrs http://cxf.apache.org/schemas/jaxrs.xsd http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

	<!-- <import resource="classpath:META-INF/cxf/cxf.xml" /> <import resource="classpath*:META-INF/cxf/cxf-extension-*.xml" 
		/> -->


	<import resource="classpath:META-INF/cxf/cxf.xml" />



	<!-- SOAP SERVICE -->
	<jaxws:endpoint address="/soapservice" implementor="#soapservice">

		<jaxws:inInterceptors>

			<bean class="org.apache.cxf.binding.soap.saaj.SAAJInInterceptor" />

			<bean class="org.apache.cxf.ws.security.wss4j.WSS4JInInterceptor">
				<constructor-arg>
					<map>
						<entry key="action" value="UsernameToken" />
						<entry key="passwordType" value="PasswordText" />
						<entry key="passwordCallbackRef">
							<ref bean="passwordCallback" />
						</entry>
					</map>
				</constructor-arg>
			</bean>
		</jaxws:inInterceptors>


	</jaxws:endpoint>




	<bean id="passwordCallback"
		class="org.uario.seaworkengine.web.services.security.PasswordCallback">
	</bean>


	<bean id="soapservice"
		class="org.uario.seaworkengine.web.services.soap.SoapControllerImpl">
		<property name="webcontroller" ref="webcontroller" />
	</bean>




	<!-- END SOAP SERVICE -->






	<!-- WEB CONTROLLER -->
	<bean id="webcontroller"
		class="org.uario.seaworkengine.web.services.webcontroller.WebControllerImpl">
		<property name="personDAO" ref="myPersonDAO"></property>
		<property name="scheduleDAO" ref="myScheduleDAO"></property>
		<property name="shiftCache" ref="myShiftCache"></property>
		<property name="taskCache" ref="myTaskCache"></property>
		<property name="ship_dao" ref="myScheduleShipDAO"></property>
		<property name="taskDAO" ref="myTasksDAO"></property>
		<property name="shipSchedulerDao" ref="myScheduleShipDAO"></property>
		<property name="configurationDAO" ref="myConfigurationDAO"></property>
		<property name="statistics" ref="myStatisticsDAO"></property>

	</bean>








</beans>