		<!-- PROGRAM -->
		<div id="program_div" visible="false">
			<popup id="shift_definition_popup" width="650px">
				<div align="right">
					<toolbarbutton label="X" style="font-weight: bold;"
						onClick="shift_definition_popup.close();errorMessageAddProgramItem.setValue(null);" />
				</div>
				<vlayout style="margin: 5px 5px 5px 5px">
					<vbox>
						<label id="alertMinHours" visible="false"
							style="color:red"
							value="Attenzione, orario programmato minore di 6 ore." />

						<separator />

						<hbox
							visible="${sec:isAnyGranted('ROLE_SUPERVISOR,ROLE_BACKOFFICE')}">

							<button id="ok_program"
								label="Salva Programmazione" sclass="btn-info">
							</button>

							<button id="cancel_program"
								label="Cancella Programmazione" sclass="btn-danger">
							</button>

						</hbox>
					</vbox>


					<div
						visible="${sec:isAnyGranted('ROLE_SUPERVISOR,ROLE_BACKOFFICE')}">

						<div id="div_force_shift">

							<separator></separator>

							<combobox id="force_shift_combo"
								placeholder="Seleziona un turno...">
							</combobox>

							<hbox align="center">
								<a label="Turno di Lavoro"
									style="color:green" id="programpopup_select_tl">
								</a>
								<a label="Riposo Programmato"
									style="color:green" id="programpopup_select_rp">
								</a>
								<a label="Giornaliero"
									style="color:green" id="programpopup_select_dl">
								</a>
							</hbox>

						</div>

					</div>
					<div id="define_program_body">


						<panel border="normal" collapsible="false"
							sclass="transparent">
							<panelchildren>

								<vbox align="start">
									<a id="scheduler_label"></a>
									<a id="label_date_shift_program"
										sclass="header" iconSclass="z-icon-check" />
									<a id="editor_label" />
									<a id="controller_label" />
									<label id="totalHours_Program"
										value="Totale Ore Programmate: " />
								</vbox>

								<hbox align="center"
									id="day_shift_over_program">
									<a
										label="Inizia il Giorno seguente: ">
									</a>
									<checkbox checked="false"
										id="day_shift_over_control_program_init">
									</checkbox>
									<a
										label="Finisce il Giorno seguente: ">
									</a>
									<checkbox checked="true"
										id="day_shift_over_control_program">
									</checkbox>
								</hbox>

								<separator></separator>

								<hbox align="center">

									<div
										visible="${sec:isAnyGranted('ROLE_SUPERVISOR,ROLE_BACKOFFICE')}">
										<button label="Aggiungi"
											id="add_program_item" sclass="btn-info">
										</button>

										<button label="Salva"
											visible="false" id="save_program_item" sclass="btn-info">
										</button>

										<button label="Annulla"
											visible="false" id="cancel_modify_program"
											sclass="btn-info">
										</button>

										<label
											id="errorMessageAddProgramItem" style="color:red" value="" />

									</div>



								</hbox>

								<separator></separator>

								<hbox
									visible="${sec:isAnyGranted('ROLE_SUPERVISOR,ROLE_BACKOFFICE')}">

									<label
										value="Definizione della mansione: ">
									</label>

									<combobox readonly="true"
										placeholder="Mansioni" id="program_task">
									</combobox>

								</hbox>

								<separator></separator>

								<hbox
									visible="${sec:isAnyGranted('ROLE_SUPERVISOR,ROLE_BACKOFFICE')}">
									<label value="Orario di lavoro: ">
									</label>

									<timebox cols="10" format="short"
										placeholder="Ora inizio" id="time_from_program">
									</timebox>

									<timebox format="short"
										placeholder="Ora fine" cols="10" id="time_to_program">
									</timebox>

								</hbox>

								<separator></separator>


								<listbox id="listbox_program"
									sclass="domain" checkmark="true" multiple="true">


									<listhead>
										<listheader label="Mansioni" />
										<listheader
											label="Tempo programmato" />
										<listheader
											label="Orario di ingresso" />
										<listheader
											label="Orario di uscita" />
										<listheader label="" />

									</listhead>

									<listitem sclass="myCell"
										self="@{each='item_program'}" value="@{item_program}">
										<listcell>
											<label width="200px"
												value="@{item_program.task, converter='org.uario.seaworkengine.zkevent.converter.TaskConverter'}">
											</label>
										</listcell>
										<listcell>
											<label width="200px"
												value="@{item_program, converter='org.uario.seaworkengine.zkevent.converter.DecimalToTimeVacationConverter'}">
											</label>
										</listcell>
										<listcell>
											<label width="200px"
												value="@{item_program.time_from}">
											</label>
										</listcell>
										<listcell>
											<label width="200px"
												value="@{item_program.time_to}">
											</label>
										</listcell>
										<listcell>
											<toolbarbutton
												id="sw_link_edit_program" style="color:blue"
												onClick="listbox_program.setSelectedItem((Listitem)self.getParent().getParent());errorMessageAddProgramItem.setValue(null);"
												label="Modifica">
											</toolbarbutton>
											<toolbarbutton
												id="remove_program_item" style="color:blue"
												onClick="listbox_program.setSelectedItem((Listitem)self.getParent().getParent());errorMessageAddProgramItem.setValue(null);"
												label="Cancella" />
										</listcell>



									</listitem>

								</listbox>

							</panelchildren>
						</panel>

