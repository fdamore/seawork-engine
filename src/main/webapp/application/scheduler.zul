<?page id="scheduler" title="Seawork Admin"?>
<?init class="org.zkoss.zkplus.databind.AnnotateDataBinderInit"?>

<vlayout sclass="page-content" xmlns:n="native" spacing="0"
	apply="org.uario.seaworkengine.zkevent.SchedulerComposer"
	id="page_panel">

	<div sclass="page-header">
		<label value="Programma Lavoro" />
		<a label="Sistema di programmazione lavoro Seawork"
			iconSclass="z-icon-angle-double-right" sclass="subtitle" />

	</div>





	<hbox align="center">
		<a label="Seleziona data iniziale: "></a>
		<datebox id="date_init_scheduler"
			placeholder="Selezione data iniziale" width="200px">
		</datebox>

		<button id="refresh_command" label="Ricarica"
			sclass="btn-info">
		</button>
	</hbox>

	<space></space>

	<hbox align="center">
		<a label="Visualizza Versione: "></a>
		<combobox id="review" readonly="true">
			<comboitem label="PROGRAMMAZIONE" value="PROGRAMMAZIONE"
				id="program_item">
			</comboitem>
			<comboitem label="REVISIONE" value="REVISIONE"
				id="review_item">
			</comboitem>

		</combobox>

	</hbox>

	<space></space>

	<!-- Set info Scheduler per Shift -->
	<div visible="false" id="info_scheduler">

		<space></space>

		<toolbarbutton label="Chiudi" style="color:blue"
			onClick="info_scheduler.setVisible(false)">
		</toolbarbutton>


		<panel border="normal" collapsible="false"
			sclass="transparent">
			<panelchildren>

				<grid sclass="domain">
					<columns>
						<column label="Type" />
						<column label="Programmazione">
							<button id="ok_program" label="Salva"
								sclass="btn-info">
							</button>
						</column>
						<column label="Revisione">
							<button id="ok_revisione" label="Salva"
								sclass="btn-info">
							</button>
						</column>
					</columns>
					<rows>
						<row>
							<a label="Ingresso: "></a>
							<timebox></timebox>
							<timebox></timebox>
						</row>
						<row>
							<a label="Uscita: "></a>
							<timebox></timebox>
							<timebox></timebox>
						</row>
						<row>
							<a label="Assegna ore: "></a>
							<intbox constraint="no negative"
								onChange="program_combo_task.setSelectedItem(null)"
								onOK="program_combo_task.setSelectedItem(null)"
								id="time_shift_program">
							</intbox>
							<intbox constraint="no negative"
								onOK="revision_combo_task.setSelectedItem(null)"
								onChange="revision_combo_task.setSelectedItem(null)"
								id="time_shift_revision">
							</intbox>
						</row>
						<row>
							<a label="Assegna Turno: "></a>
							<combobox readonly="true"
								onChange="time_shift_program.setValue(null)"
								id="program_combo_task" placeholder="Scegli turno">
							</combobox>
							<combobox readonly="true"
								onChange="time_shift_revision.setValue(null)"
								id="revision_combo_task" placeholder="Scegli turno">
							</combobox>
						</row>
					</rows>
				</grid>

			</panelchildren>
		</panel>

		<space></space>


	</div>

