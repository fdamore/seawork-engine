<?link href="less/index.css.dsp" rel="stylesheet" type="text/css"?>

<zk>

	<zscript>
	Comparator asc = new org.uario.seaworkengine.zkevent.comparator.IdEmployComparator(true);
	Comparator dsc = new org.uario.seaworkengine.zkevent.comparator.IdEmployComparator(false);
	Comparator asc_init = new org.uario.seaworkengine.zkevent.comparator.IdEmployComparatorInitialSchedule(true);
	Comparator dsc_init = new org.uario.seaworkengine.zkevent.comparator.IdEmployComparatorInitialSchedule(false);
</zscript>






	<vlayout sclass="page-content" xmlns:n="native" spacing="0"
		apply="org.uario.seaworkengine.zkevent.SchedulerComposer"
		id="page_panel">



		<style>
			.myBottonCell {border-bottom: 1px solid; border-right: 1px
			solid; border-color:#CFCFCF;}

			.mySeparatorBorder {border-bottom: 1px solid;
			border-color:#CFCFCF;;border-right: 3px solid;
			border-color:#CFCFCF;}
		</style>


		<!--  Task List -->
		<popup id="compensation_popup">
			<panel border="none" collapsible="false"
				style="margin: 2px 2px 2px 2px">
				<panelchildren>

					<div align="right">
						<toolbarbutton label="X"
							style="font-weight: bold;" onClick="compensation_popup.close();" />
					</div>

					<hbox align="left">
						<button sclass="btn-info" label="Aggiungi"
							id="sw_link_add_comp">
						</button>

						<hbox align="left">
							<label
								value="Gestione Compensazione Annuale" />
							<a id="compe_popup_year"
								iconSclass="z-icon-angle-double-right" sclass="subtitle" />
						</hbox>

					</hbox>

					<separator></separator>

					<!-- COMP EDITOR -->
					<div id="comp_editor" visible="false"
						width="50em">


						<grid sclass="domain">
							<columns>
								<column label="Tipo" />
								<column label="Informazioni" />
							</columns>
							<rows>

								<row>
									<label
										value="Data di assegnazione:">
									</label>
									<datebox id="date_submitUser"
										placeholder="Seleziona data" width="200px" />
								</row>

								<row>
									<label
										value="Numero ore assegnate:">
									</label>
									<doublebox id="time_compUser"
										width="200px" />
								</row>

								<row>
									<label value="Note:"></label>
									<textbox id="note_compUser"
										width="200px" rows="5" />
								</row>


							</rows>
						</grid>

						<button sclass="btn-info" label="OK"
							id="sw_link_add_comp_command">
						</button>

						<separator></separator>

					</div>

					<listbox id="sw_compensation_list"
						pagingPosition="both" width="50em"
						style="border: 1px solid #CCCCCC;" checkmark="true"
						multiple="false">
						<listhead sizable="true">
							<listheader label="Data"
								sort="auto(date_submit)" />
							<listheader label="Ore Assegnate" />
							<listheader label="Note" />
							<listheader label="" />
						</listhead>

						<listitem self="@{each='item'}"
							value="@{item}">

							<listcell
								label="@{item.date_submit, converter='org.uario.seaworkengine.zkevent.converter.DateConverter'}" />

							<listcell label="@{item.time_comp }" />

							<listcell label="@{item.note }" />


							<listcell>

								<toolbarbutton id="sw_link_delete_comp"
									style="color:blue"
									onClick="sw_compensation_list.setSelectedItem((Listitem)self.getParent().getParent());"
									label="Cancella">
								</toolbarbutton>

								<toolbarbutton id="sw_link_edit_comp"
									style="color:blue"
									onClick="sw_compensation_list.setSelectedItem((Listitem)self.getParent().getParent());"
									label="Modifica">
								</toolbarbutton>

							</listcell>

						</listitem>

					</listbox>


				</panelchildren>
			</panel>
		</popup>


		<!--  Task List -->
		<popup id="task_list_popup">
			<panel border="none" collapsible="false"
				style="margin: 2px 2px 2px 2px">
				<panelchildren>
					<vbox align="center">
						<div align="right">
							<toolbarbutton label="X"
								style="font-weight: bold;" onClick="task_list_popup.close();" />
						</div>

						<a label=" " id="label_statistic_task_popup"
							iconSclass="z-icon-check" />

						<listbox sclass="domain" id="list_task_stat"
							width="20em" checkmark="false" multiple="false">

							<listhead>
								<listheader label="Mansioni Attive" />

							</listhead>

							<listitem sclass="myCell"
								self="@{each='item_task_stat'}" value="@{item_task_stat}">
								<listcell>
									<label value="@{item_task_stat}">
									</label>
								</listcell>
							</listitem>


						</listbox>

					</vbox>
				</panelchildren>
			</panel>
		</popup>


		<div sclass="page-header">

			<label value="Programma Lavoro" />
			<a label="Sistema di programmazione lavoro Sea Work"
				iconSclass="z-icon-angle-double-right" sclass="subtitle" />
		</div>

		<hbox align="center" id="last_programmer_tag">
			<label style="font-style: italic" value="Ultima chiusura " />
			<label style="font-style: italic" id="lastProgrammer" />
		</hbox>

		<separator />

		<hbox align="center" id="header_info"
			visible="${sec:isAnyGranted('ROLE_SUPERVISOR,ROLE_BACKOFFICE')}">

			<div align="left">
				<label id="loggerUserOnTable" style="font-style: italic"
					value="Nessun utente connesso." />
				<button id="switchButton" sclass="btn-info" />
			</div>


			<button label="Versione Stampabile" sclass="btn-danger"
				id="print_program_videos"
				xmlns:w="http://www.zkoss.org/2005/zk/client"
				w:onClick="window.open('scheduler.zul?printProgram=1');" />

			<a label="Scarica dati in CSV" id="overview_download"
				style="color:green" />


		</hbox>



		<button label="Stampa" sclass="btn-danger" visible="false"
			id="print_scheduler" xmlns:w="http://www.zkoss.org/2005/zk/client"
			w:onClick="window.print()" />


		<space></space>

		<hbox align="center">
			<a label="Visualizza Versione: "></a>

			<combobox id="scheduler_type_selector" readonly="true"
				visible="${sec:isAnyGranted('ROLE_SUPERVISOR,ROLE_BACKOFFICE')}">
				<comboitem label="PREVENTIVAZIONE"
					value="PREVENTIVAZIONE" id="preprocessing_item">
				</comboitem>
				<comboitem label="PROGRAMMAZIONE" value="PROGRAMMAZIONE"
					id="program_item">
				</comboitem>
				<comboitem label="CONSUNTIVO" value="PROGRAMMAZIONE"
					id="review_item">
				</comboitem>

				<comboitem label="RIEPILOGO" value="RIEPILOGO"
					id="overview_item">
				</comboitem>

			</combobox>

			<a label="Righe da visualizzare: "
				iconSclass="z-icon-angle-double-right" sclass="subtitle" />
			<intbox id="shows_rows" constraint="no negative, no zero"
				value="10">
			</intbox>

			<textbox placeholder="Cerca per Nome o Matr...."
				id="full_text_search">
			</textbox>

			<button id="refresh_command" label="Ricarica"
				sclass="btn-info" />

		</hbox>

		<div id="stat_update_command">

			<separator></separator>

			<button sclass="btn-danger" id="calculate_sat"
				label="Aggiorna Statistiche Saturazione a Video">
			</button>

			<separator></separator>

		</div>

		<!--  Shift Info Popup -->
		<popup id="shift_popup">
			<panel border="none" collapsible="false"
				style="margin: 2px 2px 2px 2px">
				<panelchildren>
					<vbox align="center">
						<div align="right">
							<toolbarbutton label="X"
								style="font-weight: bold;" onClick="shift_popup.close();" />
						</div>
						<grid width="230px">
							<columns>
								<column width="80px"></column>
								<column hflex="1"></column>
							</columns>
							<rows>
								<row>
									<label value="Codice: "
										style="font-weight:bold" />
									<label id="shift_id" />
								</row>

								<row>
									<label value="Descrizione: "
										style="font-weight:bold" />
									<label id="shift_description" />
								</row>
							</rows>
						</grid>
					</vbox>
				</panelchildren>
			</panel>
		</popup>

		<!--  Task Info Popup -->
		<popup id="task_popup">
			<panel border="none" collapsible="false"
				style="margin: 2px 2px 2px 2px">
				<panelchildren>
					<vbox align="center">
						<div align="right">
							<toolbarbutton label="X"
								style="font-weight: bold;" onClick="task_popup.close();" />
						</div>
						<grid width="230px">
							<columns>
								<column width="80px"></column>
								<column hflex="1"></column>
							</columns>
							<rows>
								<row>
									<label value="Codice: "
										style="font-weight:bold" />
									<label id="task_id" />
								</row>

								<row>
									<label value="Descrizione: "
										style="font-weight:bold" />
									<label id="task_description" />
								</row>
							</rows>
						</grid>
					</vbox>
				</panelchildren>
			</panel>
		</popup>


		<!-- STATISTIC POPUP -->
		<popup id="day_name_popup">
			<panel border="none" collapsible="false"
				style="margin: 5px 5px 5px 5px">
				<panelchildren>
					<div align="right">
						<toolbarbutton label="X"
							style="font-weight: bold;" onClick="day_name_popup.close();" />
					</div>
					<vbox>

						<a label=" " id="label_statistic_popup"
							iconSclass="z-icon-check" />

						<label id="userRoles" />
						<separator />
						<hbox align="center">

							<grid width="270px">
								<columns>
									<column width="130px"></column>
									<column width="140px"></column>
								</columns>
								<rows>

									<row>
										<label value="Reparto: " />
										<label id="userDepartment" />
									</row>

									<row>
										<label value="Saturazione: " />
										<label id="saturation" />
									</row>

									<row>
										<label
											value="Saturazione (Mese Precedente): " />
										<label id="saturation_month" />
									</row>


									<row>
										<label
											value="Lavoro domenicale: " />
										<label id="work_sunday_perc" />
									</row>

									<row>
										<label value="Festivi: " />
										<label id="work_holiday_perc" />
									</row>


									<row>
										<label
											value="Ore Settimana corrente: " />
										<label id="work_current_week" />
									</row>

									<row>
										<label
											value="Ore Mese corrente: " />
										<label id="work_current_month" />
									</row>

									<row>
										<label
											value="Ore Anno corrente: " />
										<label id="work_current_year" />
									</row>

									<row>
										<label
											value="Giorni consecutivi: " />
										<label id="working_series" />
									</row>

									<row>
										<label
											value="Lavoro su Turno 1:" />
										<label id="shift_perc_1" />
									</row>

									<row>
										<label
											value="Lavoro su Turno 2:" />
										<label id="shift_perc_2" />
									</row>

									<row>
										<label
											value="Lavoro su Turno 3:" />
										<label id="shift_perc_3" />
									</row>

									<row>
										<label
											value="Lavoro su Turno 4:" />
										<label id="shift_perc_4" />
									</row>

								</rows>

							</grid>

						</hbox>
					</vbox>
				</panelchildren>
			</panel>
		</popup>




		<!-- PREPROCESSING -->
		<div id="preprocessing_div" visible="true">


			<panel border="normal" collapsible="false" visible="false"
				id="panel_shift_period" style="margin: 5px 5px 5px 5px">
				<caption>

					<hbox align="center">
						<label value="Imposta Turni Speciale"></label>
						<a label="Chiudi" style="color:blue"
							id="show_setperiod" sclass="pull-right"
							onClick="panel_shift_period.setVisible(false)">
						</a>
					</hbox>

				</caption>


				<panelchildren>



					<vbox style="margin: 5px 5px 5px 5px">

						<hbox align="center">
							<a label="Nome Utente: "
								style="color:blue">
							</a>
							<label id="shift_period_name"></label>
						</hbox>

						<hbox align="center">
							<a label="Scelta Rapida: "
								style="color:blue">
							</a>
							<a label="Turno di Lavoro"
								style="color:green" id="shift_period_select_tl">
							</a>

							<space></space>
							<a label="Giornaliero" style="color:green"
								id="shift_period_select_dl">
							</a>
						</hbox>

						<hbox align="center">

							<combobox mold="rounded"
								id="shift_period_combo" placeholder="Segli Turno...">
							</combobox>

							<datebox placeholder="Data iniziale..."
								id="shift_period_from">
							</datebox>

							<datebox placeholder="Data finale..."
								id="shift_period_to">
							</datebox>

						</hbox>

						<button id="shift_period_ok" label="OK"
							sclass="btn-info" />


					</vbox>
				</panelchildren>
			</panel>



			<space></space>

			<hbox align="left">
				<a label="Seleziona data iniziale: "></a>

				<datebox id="date_init_scheduler"
					placeholder="Selezione data iniziale" width="200px">
				</datebox>

				<button id="go_today_preprocessing" label="Vai ad oggi"
					sclass="btn-info" />

				<a label="Imposta Turni Speciale"
					visible="${sec:isAnyGranted('ROLE_SUPERVISOR,ROLE_BACKOFFICE')}"
					id="set_panel_shift_period">
				</a>



			</hbox>

			<space></space>

			<popup id="day_definition_popup">
				<panel border="none" collapsible="false"
					style="margin: 5px 5px 5px 5px">
					<panelchildren>
						<div align="right">
							<toolbarbutton label="X"
								style="font-weight: bold;"
								onClick="day_definition_popup.close();" />
						</div>
						<vbox align="start">

							<a label="Configurazione gionaliera: "
								id="label_date_popup" sclass="header" iconSclass="z-icon-check" />
							<a label="Turno Corrente: "
								id="label_date_shift_preprocessing" sclass="header"
								iconSclass="z-icon-check" />
							<a id="editor_label_daydefinition" />
							<a id="controller_label_daydefinition" />


							<div
								visible="${sec:isAnyGranted('ROLE_SUPERVISOR,ROLE_BACKOFFICE')}">
								<separator></separator>

								<combobox id="shifts_combo_select"
									autodrop="false" width="300px" autocomplete="true"
									placeholder="Seleziona un turno..." readonly="false">
								</combobox>

								<hbox align="center">
									<a label="Turno di Lavoro"
										style="color:green" id="preprocessing_select_tl">
									</a>

									<a label="Turno di Riposo"
										style="color:green" id="preprocessing_select_rp">
									</a>

									<a label="Giornaliero"
										style="color:green" id="preprocessing_select_dl">
									</a>
								</hbox>

								<separator></separator>

							</div>


							<datebox placeholder="Fino a..."
								visible="${sec:isAnyGranted('ROLE_SUPERVISOR,ROLE_BACKOFFICE')}"
								id="day_after_config">
							</datebox>


							<separator></separator>
						</vbox>
					</panelchildren>
				</panel>

				<panel border="none" collapsible="false"
					style="margin: 5px 5px 5px 5px" title="Nota Giornaliera"
					sclass="transparent">
					<panelchildren>
						<separator></separator>

						<textbox placeholder="Inserisci nota..."
							width="300px" id="note_preprocessing" rows="5">
						</textbox>

					</panelchildren>
				</panel>

				<panel border="none" collapsible="false"
					visible="${sec:isAnyGranted('ROLE_SUPERVISOR,ROLE_BACKOFFICE')}"
					style="margin: 5px 5px 5px 5px" sclass="transparent">
					<panelchildren>
						<separator></separator>

						<button id="cancel_day_definition"
							label="Cancella Configurazione" sclass="btn-danger">
						</button>

						<button label="Salva" sclass="btn-info"
							id="ok_day_shift">
						</button>

						<separator></separator>

					</panelchildren>
				</panel>



			</popup>



